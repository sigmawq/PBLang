// Comment
// Variable declaration
var int a = 1;
var float c;

// Constant declaration
val int b = 2;

// 1d array declaration
arr int my_array;

// 2d array declaration
arr int,int my_array;

// Function declaration
def f(int a : int) {
    return a + 25;
}

// Function call + array access x2 + function call
f(1)[2][1](a + 1);

// Several variables
var int a, b;

// Explicit cast
var int d = |cast| value;

// Struct declaration
struct my_struct {
    int a;
    int b;
    double c;
}

// Struct declaration + explicit byte allocation
struct my_struct (16){
    int a;
    int b;
    double c;
}

// Assignment
variable = 1;
variable += 2;
variable *= 2*2;

// Variable declaration + type modifier
var unsigned int my_var = 1;

// Function declaration with unsigned modifier + array (input, output, multiple dim)
def foo(unsigned char c, arr int,int my_array : unsigned int){

}