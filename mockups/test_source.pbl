// Comment
// Variable declaration
var int a = 1;
var float c , d;

// Constant declaration
val int b = 2;

// 1d array declaration
arr [20, 20] int my_array;

// 2d array declaration
arr [30, 30] int my_array;

// Function declaration
def f(int a : int) {
    return a + 25;
}

// Function call + array access x2 + function call
f(1)[2][1](a + 1);

// Several variables
var int a, b;

// Explicit cast
var int d = |cast| value;

// Struct declaration
struct my_struct {
    int a;
    int b;
    double c;
}

// Struct declaration + explicit byte allocation
struct my_struct (16){
    int a;
    int b;
    double c;
}

// Assignment
variable = 1;
variable = variable +  2;
variable = variable * 2 * 2;

// Variable declaration + type modifier
var unsigned int my_var = 1;

// Function declaration with unsigned modifier + array (input, output, multiple dim)
def foo(unsigned char c, arr [50, 50] int my_array : unsigned int){

}

// User defined struct and it's use
struct user_defined_struct {
    double a;
}

var user_defined_struct my_struct = 1;

// User defined struct in function declaration
struct range{
    int a;
    int b;
}

// Multiple struct access
def range_set_default(range r : void){
    r.a.next.another = 1;
}

// Another example
def get_random_range(void : range){
    var range r, b, g;
    r.a = 200;
    r.b = 400;
    return r;
}

// Block segment
{
    var float x = 1.1;
}
