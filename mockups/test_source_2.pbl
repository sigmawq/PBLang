

def printnum(int ch, int size: void){
    for(var int idx = 0; idx < size; idx=idx+1){
        putchar(ch);
    }
}

def main(void: void){
    printf("Start");
    putchar(10);

    var int size = 10;

    for(var int idx_s = 0; idx_s < (size-1); idx_s=idx_s+1){

        printnum(32, size-(idx_s+1));
        printnum(42, idx_s+1);

        putchar(10);
    }

    printf("End");
    putchar(10);
}
